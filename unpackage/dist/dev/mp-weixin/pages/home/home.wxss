/* 水平间距 */
.ipt {
  width: 100%;
  position: fixed;
  top: 0;
  padding-bottom: 10px;
  display: flex;
  border-bottom: 1px solid #aaa;
}
.ipt text {
  font-weight: bold;
}
.ipt input {
  border: 1px solid #aaa;
  border-radius: 10px;
  margin-left: 30px;
  padding-left: 50px;
}

